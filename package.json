{
  "name": "ldap-simple-tool",
  "version": "1.0.1",
  "main": "lib/index.js",
  "bin": {
    "ldap-simple-tool": "./bin/ldap-simple-tool.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liudonghua123/ldap-simple-tool.git"
  },
  "keywords": [
    "ldap",
    "userPassword",
    "cli"
  ],
  "author": "Donghua Liu",
  "bugs": {
    "url": "https://github.com/liudonghua123/ldap-simple-tool/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "npm run build-node && npm run build-windows && npm run build-linux && npm run build-macos",
    "build-windows": "nexe -t windows bin/ldap-simple-tool.js -o build/ldap-simple-tool-windows.exe",
    "build-linux": "nexe -t linux bin/ldap-simple-tool.js -o build/ldap-simple-tool-linux",
    "build-macos": "nexe -t macos bin/ldap-simple-tool.js -o build/ldap-simple-tool-macos",
    "build-node": "ncc build bin/ldap-simple-tool.js -o build/ldap-simple-tool && shx mv build/ldap-simple-tool/index.js build/ldap-simple-tool.js && shx rm -rf build/ldap-simple-tool && shx mv build/ldap-simple-tool.js build/ldap-simple-tool"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "ldap-passwd": "^0.1.0",
    "ldapts": "^2.2.0",
    "yargs": "^15.1.0"
  },
  "pkg": {
    "scripts": [
      "*.json"
    ],
    "assets": []
  },
  "devDependencies": {
    "@zeit/ncc": "^0.21.1",
    "nexe": "^3.3.2",
    "shx": "^0.3.2"
  }
}
